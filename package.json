{
  "name": "commiracle",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev-ssl": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 next dev --turbopack --experimental-https --experimental-https-key ./certs/localhost-key.pem --experimental-https-cert ./certs/localhost.pem",
    "debug-dev-ssl": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 next dev --experimental-https --experimental-https-key ./certs/localhost-key.pem --experimental-https-cert ./certs/localhost.pem",
    "dev-fast": "next dev --turbopack",
    "dev": "cross-env NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "start:graphql": "ts-node src/server/index.ts",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.12.5",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@graphql-yoga/plugin-graphql-sse": "^3.10.10",
    "@mui/icons-material": "^6.4.0",
    "@mui/material": "^6.4.0",
    "@mui/material-nextjs": "^6.3.1",
    "@next-auth/mongodb-adapter": "^1.1.3",
    "@types/js-cookie": "^3.0.6",
    "@types/uuid": "^10.0.0",
    "apollo-server": "^3.13.0",
    "apollo-server-micro": "^3.13.0",
    "bcrypt": "^5.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "graphql": "^16.10.0",
    "graphql-subscriptions": "^3.0.0",
    "graphql-yoga": "^5.10.9",
    "js-cookie": "^3.0.5",
    "mongodb": "^6.12.0",
    "next": "15.1.4",
    "next-auth": "^4.24.11",
    "next-intl": "^3.26.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.5",
    "sass": "^1.87.0",
    "sharp": "^0.33.5",
    "uuid": "^11.0.5",
    "zustand": "^5.0.3"
